class circle:
    def __init__(self,radius=3):
        self.radius=radius
    def area(self):
        return 3.14*self.radius*self.radius
    def circumference(self):
        return 2*3.14*self.radius

c1=circle(2)
print("Area of first circl : ",c1.area(),"Circumference of first circle : ",c1.circumference())
c2=circle(3)
print("Area of first circl : ",c2.area(),"Circumference of first circle : ",c2.circumference())


